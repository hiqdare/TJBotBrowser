  <body class="ds-has-sticky-footer">
    <div class="ds-grid">
      <div class="ds-row">
        <div class="ds-col-12">
          <div class="ds-row ds-padding-bottom-5">
		  	<div class="ds-grid ds-hide ds-display-lg-block ds-tray-container ds-affix ds-full-width ds-bg-neutral-1 ds-border-b-neutral-3">
		      <div class="ds-row ds-pad-l-1 ds-pad-r-01 ds-pad-t-0_5 ds-pad-b-0_5">
		        <div class="ds-col-3">
					<div class="ds-row ds-padding-top-2">
		          		<div class="ds-flex ds-flex-align-items-center">
		            		<h4 class="ds-padding-left-2 ds-heading-3 ds-mar-b-0 ds-pad-t-0_5">TJBot Cockpit</h4>
		            		<span class="ds-badge ds-neutral ds-margin-left-0_5 ds-margin-bottom-0_5" style="text-transform:none;">beta</span>
		        		</div>
		        	</div>
				</div>
		    </div>
          </div>
        </div>
      </div>
    </div>
    <div class="ds-grid">
      <div class="ds-row ds-padding-bottom-3" id="rowbot">
        <div class="ds-col-sm-12 ds-col-md-6 ds-col-lg-4 ds-padding-bottom-xs-1 ds-hide" id="bot">
          <!--div class="ds-panel ds-panel-raised"-->
          <div class="ds-row">
            <div class="ds-col-6 thumbnail_div">
              <img src="images/bots/generic.jpeg" class="ds-img-force-grow ds-border-blue-6 tjbot" alt="TJBot Neon">
            </div>
            <div class="ds-col-6">
              <div class="ds-flex ds-flex-align_items-center">
                <p class="ds-margin-right-1 ds-margin-bottom-0">name:</p>
                <div class="ds-input-container ds-margin-right-1 ds-padding-bottom-0 ds-margin-bottom-0">
                  <input type="text" class="ds-input input-name" name="name">
                </div>
              </div>
              <div class="ds-flex ds-flex-align_items-center">
                <p class="ds-margin-right-1 ds-margin-bottom-0">location:</p>
                <div class="ds-input-container ds-margin-right-1 ds-padding-bottom-0 ds-margin-bottom-0">
                  <input type="text" class="ds-input input-location" name="location">
                </div>
              </div>
              <div class="ds-flex ds-flex-align_items-center">
                <p class="ds-margin-right-1 ds-margin-bottom-0">chocolate:</p>
                <div class="ds-input-container ds-margin-right-1 ds-padding-bottom-0 ds-margin-bottom-0">
                  <input type="text" class="ds-input input-chocolate" name="chocolate">
                </div>
              </div>
              <div class="ds-flex ds-flex-align_items-center">
                <p class="ds-margin-right-1 ds-margin-bottom-0">mentor:</p>
                <div class="ds-input-container ds-margin-right-1 ds-padding-bottom-0 ds-margin-bottom-0">
                  <input type="text" class="ds-input input-mentor" name="mentor">
                </div>
              </div>
              <div class="ds-flex ds-flex-align_items-center">
                <div class="ds-row">
                  <div class="ds-col-xs-12">
                    <div class="ds-dropdown ds-flat ds-margin-bottom-0" tabindex="0" role="menu" aria-label="w3DS Flat Dropdown">
                        <div class="ds-title ds-margin-bottom-0">image</div>
                        <div class="ds-options botImageList" role="group">
                          <div class="ds-option" role="menuitem">
                              option 1
                          </div>
                        </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="ds-flex ds-flex-align_items-center">
                <span class="ds-margin-right-1 ds-icon-wireless status"></span>
                <i class="ds-margin-right-1 far fa-lightbulb bot-led" alt="LED" data-toggle="tooltip" title="LED"></i>
                <i class="ds-margin-right-1 far fa-hand-paper bot-arm" alt="arm" data-toggle="tooltip" title="arm"></i>
              </div>
            </div>
          </div>
          <div class="ds-row">
            <div class="ds-col-11">
              <div class="ds-accordion-container ds-margin-top-0 ds-margin-bottom-0">
                <div class="ds-row ds-accordion-item" aria-controls="ds-accordion-slidedown" aria-expanded="false" role="accordion item">
                  <div class="ds-accordion-title">Services</div>
                  <div class="ds-accordion-slidedown" aria-hidden="true" role="region">
                    <div class="ds-row">
                      <div class="ds-col-xs-11">
                        <h4 class="ds-heading-4">Text-to-Speech</h4>
                      </div>
                        <div class="ds-col-xs-11">
                            <div class="ds-dropdown" tabindex="0" role="menu" aria-label="w3DS Auto Width Dropdown">
                                <div class="ds-title" role="menu-title">Choose your voice</div>
                                <div class="ds-options voicesList" role="group">
                                </div>
                            </div>
                        </div>
                    </div>
                  </div>
                </div>
					      <div class="ds-row ds-accordion-item" aria-controls="ds-accordion-slidedown" aria-expanded="false" role="accordion item">
						      <div class="ds-accordion-title">Data</div>
                  <div class="ds-accordion-slidedown" aria-hidden="true" role="region">
                    <div class="ds-col-8">
                      <div class="ds-flex ds-flex-align_items-center">
                        <span class="ds-margin-right-1 far fa-file-code" alt="update" data-toggle="tooltip" title="Source code"></span>
                        <p class="ds-margin-right-1 source_version">-</p>
                        <span class="fas fa-recycle source_update" alt="update" data-toggle="tooltip" title="Update Source code" style="margin-left: auto;"></p>
                      </div>
                      <div class="ds-flex ds-flex-align_items-center">
                        <img src="images/nodejs.png" class="ds-margin-right-1 logo_mini" alt="node.js" data-toggle="tooltip" title="node.js">
                        <p class="ds-margin-right-1 nodejs_version">-</p>
                        <span class="fas fa-recycle nodejs_update" alt="update" data-toggle="tooltip" title="Update node.js" style="margin-left: auto;"></p>
                      </div>
                      <div class="ds-flex ds-flex-align_items-center">
                        <img src="images/npm.png" class="ds-margin-right-1 logo_mini" alt="npm" data-toggle="tooltip" title="npm">
                        <p class="ds-margin-right-1 npm_version">-</p>
                        <span class="fas fa-recycle npm_update" alt="update" data-toggle="tooltip" title="Update npm" style="margin-left: auto;"></p>
                      </div>
                      <div class="ds-flex ds-flex-align_items-center">
                        <img src="images/raspbian.png" class="ds-margin-right-1 logo_mini" alt="os version" data-toggle="tooltip" title="os version">
                        <p class="ds-margin-right-1 os_version">-</p>
                      </div>
                      <div class="ds-flex ds-flex-align_items-center">
                        <img src="images/raspberry_logo.png" class="ds-margin-right-1 logo_mini" alt="firmware version" data-toggle="tooltip" title="firmware version">
                        <p class="ds-margin-right-1 firmware">-</p>
                      </div>
                    </div>
						      </div>
					      </div>
				      </div>
            </div>
          </div>
		  </div>
	  </div>
	</div>

    <footer class="ds-sticky-footer ds-grid ds-bg-neutral-warm-2" style="width: 100%; height: 110px;" >
